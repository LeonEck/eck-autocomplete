{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2022",
    "moduleResolution": "Node",
    "allowJs": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["eck-autocomplete/**/*.ts", "types"],
  "exclude": ["vitest.config.ts", "eck-autocomplete/**/*.test.ts"]
}
