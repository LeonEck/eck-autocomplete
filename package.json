{
  "name": "eck-autocomplete",
  "version": "0.1.0",
  "description": "Autocomplete web component",
  "author": "Leon Eck",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "type-check": "tsc --noEmit",
    "generate-type-declarations": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "watch": "node scripts/esbuild-watch.mjs",
    "prettier": "prettier --ignore-path .gitignore --write .",
    "prettier-check": "prettier --ignore-path .gitignore --check .",
    "eslint-check": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "stylelint-check": "stylelint --ignore-path .gitignore **/*.css",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "esbuild": "0.14.14",
    "eslint": "8.8.0",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "stylelint": "14.3.0",
    "stylelint-config-standard": "24.0.0",
    "typescript": "4.5.5",
    "vite": "2.7.10"
  }
}
