{
  "name": "eck-autocomplete",
  "version": "1.1.1",
  "description": "Autocomplete web component. Suggests options for an input.",
  "author": "Leon Eck",
  "license": "MIT",
  "web-types": "./web-types.json",
  "type": "module",
  "engines": {
    "node": "^20.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonEck/eck-autocomplete.git"
  },
  "homepage": "https://eck-autocomplete.leoneck.de",
  "scripts": {
    "start": "concurrently -r npm:watch:*",
    "watch:library": "node scripts/esbuild-library-watch.js",
    "watch:e2e": "vite --host 0.0.0.0",
    "dev:showcase": "vitepress dev showcase --open",
    "dev:storybook": "storybook dev -p 6006",
    "build:library": "node scripts/esbuild-library-production-build.js",
    "build:showcase": "vitepress build showcase && node scripts/copy-e2e-to-www.js",
    "build:storybook": "storybook build -o www/storybook",
    "build:cloudflare": "npm run build:library && npm run build:showcase && npm run build:storybook",
    "preview:storybook:prod": "npx http-server www -o /storybook/",
    "generate:type-declarations": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "generate:package-metadata": "node scripts/generate-package-metadata.js",
    "bundle:library": "npm run build:library && npm run generate:type-declarations && npm run generate:package-metadata",
    "pack:library": "cd dist && npm pack",
    "publish:library": "npm run bundle:library && cd dist && npm publish",
    "check:types": "tsc --noEmit",
    "check:ci:types": "typescript_version=$(grep '\"typescript\"' package.json | tail -n 1 | sed -r 's/.*:.*\"(.*)\",$/\\1/') && npx -p typescript@$typescript_version tsc --noEmit",
    "check:eslint": "eslint --max-warnings 0 .",
    "check:stylelint": "stylelint --ignore-path .gitignore **/*.scss",
    "check:prettier": "prettier --ignore-path .gitignore --check .",
    "check:ci:prettier": "prettier_version=$(grep '\"prettier\"' package.json | tail -n 1 | sed -r 's/.*:.*\"(.*)\",$/\\1/') && npx prettier@$prettier_version --ignore-path .gitignore --check .",
    "fix:prettier": "prettier --ignore-path .gitignore --write .",
    "test:e2e": "start-server-and-test test:e2e:start http-get://127.0.0.1:5173 test:e2e:playwright:test",
    "test:e2e:start": "npm run build:library && npm run watch:e2e",
    "test:e2e:playwright:test": "playwright test",
    "test:e2e:playwright:test:ui": "playwright test --ui",
    "test:e2e:playwright:install": "playwright install --with-deps chromium chrome msedge firefox webkit",
    "test:unit": "vitest run",
    "test:unit:vitest:watch": "vitest --ui",
    "test:backstop": "start-server-and-test test:e2e:start http-get://127.0.0.1:5173 test:backstop:test",
    "test:backstop:test": "backstop reference && backstop test",
    "test:backstop:install:chromium": "playwright install --with-deps chromium",
    "test:backstop:install:firefox": "playwright install --with-deps firefox",
    "test:backstop:install:webkit": "playwright install --with-deps webkit",
    "report:size:library": "node scripts/report-size-library.js"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@playwright/test": "1.43.1",
    "@storybook/addon-actions": "8.0.9",
    "@storybook/addon-essentials": "8.0.9",
    "@storybook/addon-links": "8.0.9",
    "@storybook/addon-webpack5-compiler-swc": "1.0.2",
    "@storybook/html": "8.0.9",
    "@storybook/html-webpack5": "8.0.9",
    "@vitest/ui": "1.5.3",
    "backstopjs": "6.3.23",
    "concurrently": "8.2.2",
    "esbuild": "0.20.2",
    "esbuild-sass-plugin": "3.2.0",
    "eslint": "8.57.0",
    "eslint-plugin-storybook": "0.8.0",
    "globals": "15.1.0",
    "happy-dom": "14.7.1",
    "html-minifier-terser": "7.2.0",
    "html-webpack-plugin": "5.6.0",
    "prettier": "3.2.5",
    "start-server-and-test": "2.0.3",
    "storybook": "8.0.9",
    "stylelint": "16.4.0",
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-config-recommended-scss": "14.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0",
    "vite": "5.2.10",
    "vitepress": "1.1.4",
    "vitest": "1.5.3"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.3"
  }
}
