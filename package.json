{
  "name": "eck-autocomplete",
  "version": "0.1.0",
  "description": "Autocomplete web component",
  "author": "Leon Eck",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "type-check": "tsc --noEmit",
    "generate-type-declarations": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "watch": "node scripts/esbuild-watch.mjs",
    "prettier": "prettier --ignore-path .gitignore --write .",
    "prettier-check": "prettier --ignore-path .gitignore --check ."
  },
  "devDependencies": {
    "esbuild": "0.14.14",
    "prettier": "2.5.1",
    "typescript": "4.5.5",
    "vite": "2.7.10"
  }
}
