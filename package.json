{
  "name": "eck-autocomplete",
  "version": "0.1.1",
  "description": "Autocomplete web component. Suggests options for an input.",
  "author": "Leon Eck",
  "license": "MIT",
  "engines": {
    "node": "^16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonEck/eck-autocomplete.git"
  },
  "homepage": "https://eck-autocomplete.leoneck.de",
  "scripts": {
    "start": "concurrently -r npm:watch:*",
    "watch:library": "node scripts/esbuild-library-watch.mjs",
    "watch:e2e": "vite --host 0.0.0.0",
    "dev:showcase": "vuepress dev showcase",
    "dev:storybook": "start-storybook -p 6006",
    "build:library": "node scripts/esbuild-library-production-build.mjs",
    "build:showcase": "vuepress build showcase && node scripts/copy-e2e-to-www.mjs",
    "build:storybook": "build-storybook -o www/storybook",
    "build:cloudflare": "npm run build:library && npm run build:showcase && npm run build:storybook",
    "generate:type-declarations": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "generate:package-metadata": "node scripts/generate-package-metadata.mjs",
    "bundle:library": "npm run build:library && npm run generate:type-declarations && npm run generate:package-metadata",
    "pack:library": "cd dist && npm pack",
    "publish:library": "npm run bundle:library && cd dist && npm publish",
    "check:types": "tsc --noEmit",
    "check:ci:types": "typescript_version=$(grep '\"typescript\"' package.json | tail -n 1 | sed -r 's/.*:.*\"(.*)\",$/\\1/') && npx -p typescript@$typescript_version tsc --noEmit",
    "check:eslint": "eslint --ignore-path .gitignore --max-warnings 0 .",
    "check:stylelint": "stylelint --ignore-path .gitignore **/*.scss",
    "check:prettier": "prettier --ignore-path .gitignore --check .",
    "check:ci:prettier": "prettier_version=$(grep '\"prettier\"' package.json | tail -n 1 | sed -r 's/.*:.*\"(.*)\",$/\\1/') && npx prettier@$prettier_version --ignore-path .gitignore --check .",
    "fix:prettier": "prettier --ignore-path .gitignore --write .",
    "test:e2e": "start-server-and-test test:e2e:start http-get://localhost:3000 test:e2e:playwright:test",
    "test:e2e:start": "npm run build:library && npm run watch:e2e",
    "test:e2e:playwright:test": "playwright test",
    "test:e2e:playwright:install": "playwright install --with-deps chromium chrome msedge firefox webkit",
    "test:unit": "vitest run",
    "test:unit:vitest:watch": "vitest",
    "test:backstop": "start-server-and-test test:e2e:start http-get://localhost:3000 test:backstop:test",
    "test:backstop:test": "backstop reference && backstop test",
    "test:backstop:install:chromium": "playwright install --with-deps chromium",
    "test:backstop:install:firefox": "playwright install --with-deps firefox",
    "test:backstop:install:webkit": "playwright install --with-deps webkit",
    "report:size:library": "node scripts/report-size-library.mjs",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@commitlint/cli": "16.2.3",
    "@playwright/test": "1.21.1",
    "@storybook/addon-actions": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/html": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "@vuepress/plugin-register-components": "2.0.0-beta.38",
    "@vuepress/plugin-shiki": "2.0.0-beta.38",
    "babel-loader": "8.2.5",
    "backstopjs": "6.0.4",
    "concurrently": "7.1.0",
    "esbuild": "0.14.38",
    "esbuild-sass-plugin": "2.2.6",
    "eslint": "8.13.0",
    "happy-dom": "3.1.0",
    "html-minifier": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "prettier": "2.6.2",
    "start-server-and-test": "1.14.0",
    "stylelint": "14.7.1",
    "stylelint-config-property-sort-order-smacss": "9.0.0",
    "stylelint-config-recommended-scss": "6.0.0",
    "typescript": "4.6.3",
    "vite": "2.9.5",
    "vitest": "0.9.4",
    "vuepress": "2.0.0-beta.39"
  },
  "dependencies": {
    "@floating-ui/dom": "0.4.5"
  }
}
